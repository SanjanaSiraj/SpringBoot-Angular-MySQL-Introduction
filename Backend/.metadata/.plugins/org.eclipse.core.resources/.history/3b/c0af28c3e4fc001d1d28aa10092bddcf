package com.example.demo.service;

import java.util.List;

import org.springframework.stereotype.Component;

import com.example.demo.entity.Book;


@Component
public class BookService {
	public List<Book> getBooks(){
		return List.of(
			new Book(
					"title",
					0.0,
					2023,
					"author",
					"genre",
					"publisher")	
			);
	}
}
